  "devDependencies": {
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "jsdoc": "^4.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parthakonda/gs-orm.git"
